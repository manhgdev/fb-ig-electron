{
  "name": "fbaio",
  "author": "TOOLFB.VN",
  "version": "5.2.7",
  "description": "FB AIO",
  "license": "ISC",
  "main": "main.js",
  "build": {
    "appId": "com.fbaio.app",
    "productName": "FB All in One",
    "win": {
      "icon": "fbaio.ico",
      "target": "nsis",
      "artifactName": "FB_All_in_One_Setup.${ext}",
      "requestedExecutionLevel": "requireAdministrator",
      "publish": [
        {
          "provider": "generic",
          "url": "https://toolfb.vn/update",
          "useMultipleRangeRequest": false
        }
      ]
    },
    "afterPack": "afterPack.js",
    "beforePack": "beforePack.js",
    "files": [
      "!tests${/*}",
      "!backup${/*}",
      "!upload.js"
    ],
    "nsis": {
      "installerIcon": "fbaio.ico",
      "uninstallerIcon": "fbaio.ico",
      "oneClick": false,
      "perMachine": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "runAfterFinish": true
    },
    "directories": {
      "output": "releases"
    },
    "asarUnpack": [
      "ext"
    ]
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder & node upload & git stash",
    "build": "webpack",
    "watch": "webpack --watch"
  },
  "devDependencies": {
    "asarmor": "^2.1.0-beta.10",
    "clean-webpack-plugin": "^4.0.0",
    "electron": "^24.4.0",
    "electron-builder": "^24.6.4",
    "javascript-obfuscator": "^4.0.2"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "branchy": "^2.0.0",
    "charset-detector": "^0.0.2",
    "cheerio": "^1.0.0-rc.12",
    "chrome-finder": "^1.0.7",
    "crypto": "^1.0.1",
    "csv": "^6.3.5",
    "currency-converter-lt": "^1.3.1",
    "electron-builder-squirrel-windows": "^24.6.3",
    "electron-log": "^4.4.8",
    "electron-updater": "^6.1.1",
    "font-list": "^1.5.0",
    "form-data": "^4.0.0",
    "fs-extra": "^11.1.1",
    "generate-password": "^1.7.0",
    "html-entities": "^2.5.2",
    "https-proxy-agent": "^7.0.1",
    "imap-simple": "^1.6.3",
    "imapflow": "^1.0.164",
    "languagedetect": "^2.0.0",
    "lorem-ipsum": "^2.0.8",
    "mailparser": "^3.7.1",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "node-2fa": "^2.0.3",
    "node-fetch": "^2.6.12",
    "node-machine-id": "^1.1.12",
    "nodemailer": "^6.9.6",
    "promise-limit": "^2.7.0",
    "puppeteer-core": "^20.3.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "qrcode": "^1.5.3",
    "quoted-printable": "^1.0.1",
    "sftp-upload": "^1.0.4",
    "sharp": "^0.32.2",
    "systeminformation": "^5.18.6",
    "tree-kill": "^1.2.2",
    "tweetnacl-sealedbox-js": "^1.2.0",
    "unique-username-generator": "^1.2.0",
    "uuid": "^9.0.1",
    "whatsapp-web.js": "github:pedroslopez/whatsapp-web.js#webpack-exodus"
  }
}
